{
  "name": "@mysticatea/tools",
  "version": "0.0.0",
  "description": "My tools",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12.10.0"
  },
  "bin": {
    "release": "bin/release.js"
  },
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib"
  ],
  "dependencies": {},
  "devDependencies": {
    "@mysticatea/eslint-plugin": "^13.0.0",
    "@types/node": "^14.0.13",
    "cpx": "^1.5.0",
    "eslint": "^7.3.0",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json && cpx \"{LICENSE,package.json,README.md}\" dist",
    "clean": "rimraf .nyc_output coverage dist",
    "test": "eslint . && tsc -p tsconfig.build.json --noEmit && nyc mocha",
    "preversion": "npm test",
    "version": "npm run -s build",
    "postversion": "ts-node bin/release.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mysticatea/tools.git"
  },
